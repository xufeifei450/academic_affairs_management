      <el-button plain type=\"primary\" size=\"mini\" @click=\"initComment(scope.row)\" :disabled=\"scope.row.status !== '已结课'\">评教</el-button>\r\n                    </template>\r\n                </el-table-column>\r\n            </el-table>\r\n\r\n            <div class=\"pagination\">\r\n                <el-pagination\r\n                        background\r\n                        @current-change=\"handleCurrentChange\"\r\n                        :current-page=\"pageNum\"\r\n                        :page-sizes=\"[5, 10, 20]\"\r\n                        :page-size=\"pageSize\"\r\n                        layout=\"total, prev, pager, next\"\r\n                        :total=\"total\">\r\n                </el-pagination>\r\n            </div>\r\n        </div>\r\n        <el-dialog title=\"请填写评教信息\" :visible.sync=\"fromVisible\" width=\"40%\" :close-on-click-modal=\"false\" destroy-on-close>\r\n            <el-form label-width=\"100px\" style=\"padding-right: 50px\" :model=\"form\" :rules=\"rules\" ref=\"formRef\">\r\n                <el-form-item prop=\"content\" label=\"评教内容\">\r\n                    <el-input type=\"textarea\" :rows=\"5\" v-model=\"form.content\" autocomplete=\"off\"></el-input>\r\n                </el-form-item>\r\n            </el-form>\r\n            <div slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"fromVisible = false\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n            </div>\r\n        </el-dialog>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        name: \"Choice\",\r\n        data() {\r\n            return {\r\n                tableData: [],  // 所有的数据\r\n                pageNum: 1,   // 当前的页码\r\n                pageSize: 10,  // 每页显示的个数\r\n                total: 0,\r\n                user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n                rules: {\r\n                    content:[\r\n                        {required:true,message:'请输入内容',trigger:'blur'},\r\n                    ]\r\n                },\r\n                ids: [],\r\n                form:{},\r\n            }\r\n        },\r\n        created() {\r\n            this.load(1)\r\n        },\r\n        methods: {\r\n            initComment(row){\r\n              this.form=JSON.parse(JSON.stringify(row))\r\n                this.fromVisible=true\r\n            },\r\n            save(){\r\n                let data={}\r\n                this.$request.post('/comment/add',data)\r\n            },\r\n            del(id) {   // 单个删除\r\n                this.$confirm('您确定取消选这门课吗？这个老师的课不好选哦！！', '灵魂拷问', {type: \"warning\"}).then(response => {\r\n                    this.$request.delete('/choice/delete/' + id).then(res => {\r\n                        if (res.code === '200') {   // 表示操作成功\r\n                            this.$message.success('操作成功')\r\n                            this.load(1)\r\n                        } else {\r\n                            this.$message.error(res.msg)  // 弹出错误的信息\r\n                        }\r\n                    })\r\n                }).catch(() => {\r\n                })\r\n            },\r\n            load(pageNum) {  // 分页查询\r\n                if (pageNum) this.pageNum = pageNum\r\n                this.$request.get('/choice/selectPage', {\r\n                    params: {\r\n                        pageNum: this.pageNum,\r\n                        pageSize: this.pageSize,\r\n                        name: this.name,\r\n                    }\r\n                }).then(res => {\r\n                    this.tableData = res.data?.list\r\n                    this.total = res.data?.total\r\n                })\r\n            },\r\n            reset() {\r\n                this.name = null\r\n                this.load(1)\r\n            },\r\n            handleCurrentChange(pageNum) {\r\n                this.load(pageNum)\r\n            },\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":"AAoDA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,KAAA;QACAC,OAAA,GACA;UAAAC,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MAEA;MACAC,GAAA;MACAC,IAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAC,YAAAC,GAAA;MACA,KAAAL,IAAA,GAAAV,IAAA,CAAAC,KAAA,CAAAD,IAAA,CAAAgB,SAAA,CAAAD,GAAA;MACA,KAAAE,WAAA;IACA;IACAC,KAAA;MACA,IAAAxB,IAAA;MACA,KAAAyB,QAAA,CAAAC,IAAA,iBAAA1B,IAAA;IACA;IACA2B,IAAAC,EAAA;MAAA;MACA,KAAAC,QAAA;QAAAC,IAAA;MAAA,GAAAC,IAAA,CAAAC,QAAA;QACA,KAAAP,QAAA,CAAAQ,MAAA,qBAAAL,EAAA,EAAAG,IAAA,CAAAG,GAAA;UACA,IAAAA,GAAA,CAAAC,IAAA;YAAA;YACA,KAAAC,QAAA,CAAAC,OAAA;YACA,KAAAnB,IAAA;UACA;YACA,KAAAkB,QAAA,CAAAE,KAAA,CAAAJ,GAAA,CAAAK,GAAA;UACA;QACA;MACA,GAAAC,KAAA,QACA;IACA;IACAtB,KAAAhB,OAAA;MAAA;MACA,IAAAA,OAAA,OAAAA,OAAA,GAAAA,OAAA;MACA,KAAAuB,QAAA,CAAAgB,GAAA;QACAC,MAAA;UACAxC,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAJ,IAAA,OAAAA;QACA;MACA,GAAAgC,IAAA,CAAAG,GAAA;QACA,KAAAjC,SAAA,GAAAiC,GAAA,CAAAlC,IAAA,EAAA2C,IAAA;QACA,KAAAvC,KAAA,GAAA8B,GAAA,CAAAlC,IAAA,EAAAI,KAAA;MACA;IACA;IACAwC,MAAA;MACA,KAAA7C,IAAA;MACA,KAAAmB,IAAA;IACA;IACA2B,oBAAA3C,OAAA;MACA,KAAAgB,IAAA,CAAAhB,OAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}